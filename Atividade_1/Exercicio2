org 00h
inicio:
	mov ACC,#2 ;insere o valor dois ao acc
	mov B,#3   ;insere o valor 3 no b
	mov R0,#7  ;r0 recebe o valor 7
	add a,r0   ;
	dec a 	   ;decrementar a
	dec a	   ;decrementar a
 	dec a 	   ;decrementar a
	inc b	   ;incrementar b
	subb a,b
	mul ab
	inc b	   ;incrementar b
	inc b	   ;incrementar b
	div ab
	mov R1,a
	mov R2,b
	jmp inicio
